{
  "name": "ai-consul",
  "version": "1.0.0",
  "description": "Privacy-first, real-time AI assistant for conversations, interviews, and meetings",
  "main": "dist/main/main.js",
  "scripts": {
    "dev": "concurrently \"pnpm run dev:main\" \"pnpm run dev:renderer\"",
    "dev:main": "tsc -p tsconfig.main.json && DEBUG_AUDIO=true electron .",
    "dev:renderer": "vite",
    "build": "pnpm run build:main && pnpm run build:renderer",
    "validate:deps": "node scripts/validate-dependencies.js",
    "analyze:deps": "tsx scripts/analyze-deps.ts",
    "analyze:bundle": "ANALYZE=true pnpm run build",
    "build:local-only": "INCLUDE_CLOUD=false BUNDLE_MODELS=true pnpm run build",
    "build:balanced": "INCLUDE_CLOUD=true BUNDLE_MODELS=false pnpm run build",
    "build:full": "INCLUDE_CLOUD=true BUNDLE_MODELS=true pnpm run build",
    "build:with-models": "BUNDLE_MODELS=true pnpm run build",
    "build:main": "tsc -p tsconfig.main.json && tsc -p tsconfig.preload.json",
    "build:renderer": "vite build",
    "build:mac": "pnpm run build && electron-builder --mac",
    "build:win": "pnpm run build && electron-builder --win",
    "build:variants": "bash scripts/build-variants.sh",
    "notarize": "electron-builder --mac --publish never",
    "make": "electron-builder",
    "test:packaging": "pnpm run build && pnpm run make && pnpm run test:package-size",
    "test:package-size": "node scripts/check-package-size.js",
    "optimize:deps": "pnpm prune --prod && pnpm dedupe",
    "clean:deps": "rm -rf node_modules pnpm-lock.yaml && pnpm install",
    "lint": "eslint . --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "benchmark": "tsx scripts/benchmark.ts",
    "benchmark:local": "tsx scripts/benchmark.ts --modes local-only,local-first",
    "validate:wer": "tsx scripts/validate-wer.ts"
  },
  "keywords": [
    "ai",
    "assistant",
    "electron",
    "privacy",
    "real-time"
  ],
  "author": "CriticalSuccess",
  "license": "MIT",
  "dependencies": {
    "@types/form-data": "^2.5.2",
    "@xenova/transformers": "^2.17.2",
    "assemblyai": "^4.0.0",
    "axios": "^1.7.7",
    "@deepgram/sdk": "^4.4.0",
    "dotenv": "^16.4.5",
    "electron-updater": "^6.1.7",
    "form-data": "^4.0.4",
    "mammoth": "^1.7.0",
    "node-wav": "^0.0.2",
    "onnxruntime-node": "^1.16.0",
    "pcm-convert": "^1.6.5",
    "pdf-parse": "^1.1.1",
    "python-shell": "^5.0.0",
    "react-router-dom": "^7.9.5",
    "ws": "^8.14.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.19.24",
    "@types/pdf-parse": "^1.1.5",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^4.0.6",
    "@vitest/ui": "^4.0.6",
    "concurrently": "^8.2.2",
    "electron": "^30.0.0",
    "electron-builder": "^24.13.3",
    "eslint": "^8.57.0",
    "jsdom": "^27.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.5.3",
    "vite": "^5.3.1",
    "vitest": "^4.0.6",
    "yaml": "^2.4.5",
    "tsx": "^4.19.2",
    "zustand": "^4.5.2"
  }
}
